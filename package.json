{
  "name": "@npm/pg-db-session",
  "version": "1.1.0",
  "description": "domain-attached database sessions",
  "main": "db-session.js",
  "scripts": {
    "test:code": "tap test/*-test.js",
    "test:style": "standard",
    "test": "npm run test:code && npm run test:style",
    "cov:test": "nyc npm run test:code",
    "cov:html": "npm run cov:test && nyc report --reporter=lcov",
    "cov:view": "npm run cov:html && ecstatic coverage/lcov-report -p 60888"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/npm/pg-db-session.git"
  },
  "keywords": [
    "postgres",
    "database",
    "domain",
    "session"
  ],
  "author": "Chris Dickinson <chris@neversaw.us> (http://neversaw.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/npm/pg-db-session/issues"
  },
  "homepage": "https://github.com/npm/pg-db-session#readme",
  "devDependencies": {
    "ecstatic": "^1.4.0",
    "nyc": "^5.3.0",
    "pg": "^4.4.3",
    "standard": "^5.4.1",
    "tap": "^5.0.0"
  },
  "dependencies": {
    "bluebird": "^3.1.1"
  }
}
